{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ekko",
  "version": "0.1.0",
  "identifier": "de.ekko.ai",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "test",
        "title": "ekko",
        "width": 500,
        "height": 500,
        "transparent": false,
        "center": true,
        "decorations": true,
        "url": "test.html"
      },
      {
        "label": "main",
        "title": "ekko",
        "width": 500,
        "height": 500,
        "x": 0,
        "transparent": false,
        "decorations": true,
        "url": "test.html"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
